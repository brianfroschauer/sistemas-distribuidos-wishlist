# Index
GET        /                     controllers.HomeController.index

POST       /api/products         controllers.UserController.addProductToUser
GET        /api/products/:id     controllers.UserController.getProductsFromUserWith(id: Long)
GET        /api/products-description/:id     controllers.UserController.getProductsFromUserWithDescription(id: Long)

POST       /api/users            controllers.UserController.addUser


# ----     gRPC services    ----
->        /                  routers.HelloWorldRouter
# ---- end of gRPC services ----
